{
  "name": "amen",
  "version": "1.0.0-alpha-04",
  "description": "A simple test suite for use with promises and generators",
  "keywords": [
    "test",
    "asynchronous",
    "promises",
    "generators"
  ],
  "main": "lib/amen.js",
  "scripts": {
    "prepublish": "coffee --nodejs --harmony -o lib/ -c src/*.*coffee",
    "test": "coffee --nodejs --harmony test/index.coffee",
    "watch": "coffee --nodejs --harmony -o lib/ -cw src/*.*coffee"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pandastrike/amen.git"
  },
  "authors": [
    {
      "name": "Dan Yoder",
      "email": "dan@pandastrike.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pandastrike/amen/issues"
  },
  "homepage": "https://github.com/pandastrike/amen",
  "dependencies": {
    "colors": "^0.6.2",
    "when": "^3.4.6"
  },
  "devDependencies": {
    "coffee-script": "git://github.com/jashkenas/coffeescript.git#4f82e59"
  },
  "engine": "node >= 0.11.x"
}
