{
  "name": "amen",
  "version": "1.0.2",
  "description": "A simple test suite for use with promises and generators",
  "keywords": [
    "test",
    "asynchronous",
    "promises",
    "generators"
  ],
  "files": [
    "lib/",
    "LICENSE"
  ],
  "main": "lib/amen.js",
  "scripts": {
    "test": "coffee test/index.coffee",
    "watch": "coffee -o lib/ -cw src/*.*coffee",
    "prepublish": "coffee -o lib/ -c src/*.*coffee",
    "postpublish": "(node_modules/.bin/json -f package.json version | xargs -I version git tag -am version version) && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pandastrike/amen.git"
  },
  "authors": [
    {
      "name": "Dan Yoder",
      "email": "dan@pandastrike.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pandastrike/amen/issues"
  },
  "homepage": "https://github.com/pandastrike/amen",
  "dependencies": {
    "colors": "^0.6.2",
    "fairmont": "1.1.x"
  },
  "devDependencies": {
    "coffee-script": "git://github.com/jashkenas/coffeescript.git#4f82e59",
    "json": "^9.0.3"
  },
  "engine": "node >= 6.0.0"
}
